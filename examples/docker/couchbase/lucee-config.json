      "version": "1.0.0.0"
    },
    {
      "id": "8D7FB0DF-08BB-1589-FE3975678F07DB17",
      "name": "Compress Tags",
      "version": "2.0.0.1-SNAPSHOT"
    },
    {
      "name": "Couchbase driver",
      "version": "1.0.0.35-SNAPSHOT",
      "url": "https://ext.lucee.org/lucee.extension.couchbase-1.0.0.35-SNAPSHOT.lex"
    }
  ],
  "jdbcDrivers": {
    "com.mysql.cj.jdbc.Driver": {
      "label": "MySQL",
      "connectionString": "jdbc:mysql://{host}:{port}/{database}",
      "id": "mysql",
      "bundleName": "com.mysql.cj",
      "bundleVersion": "9.3.0"
    },
    "com.microsoft.sqlserver.jdbc.SQLServerDriver": {
      "label": "Microsoft SQL Server (Vendor Microsoft)",
      "connectionString": "jdbc:sqlserver://{host}:{port}",
      "id": "mssql",
      "bundleName": "org.lucee.mssql",
      "bundleVersion": "12.6.3.jre11"
    },
    "org.postgresql.Driver": {
      "label": "PostgreSQL",
      "connectionString": "jdbc:postgresql://{host}:{port}/{database}",
      "id": "postgresql",
      "bundleName": "org.postgresql.jdbc",
      "bundleVersion": "42.7.7"
    },
    "net.sourceforge.jtds.jdbc.Driver": {
      "label": "jTDS (MSSQL,Sybase)",
      "connectionString": "jdbc:jtds:sqlserver://{host}:{port}/{database}",
      "id": "jtds",
      "bundleName": "jtds",
      "bundleVersion": "1.3.1"
    }
  }
}